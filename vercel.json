{
  "builds": [
    {
      "src": "index.php",
      "use": "@vercel/php"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.php"
    }
  ],
  "functions": {
    "api/*.php": {
      "runtime": "vercel-php@8.1.2"
    }
  },
  "public": "public",
  "ignore": [
    "vendor"
  ]
}
